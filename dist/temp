class Trie:
	head = {}

	def add(self, word):
		cur = self.head
		for ch in word:
			if ch not in cur:
				cur[ch] = {}
			cur = cur[ch]
		cur['*'] = True

	def startsWith(self, prefix):
		current = self.head        
		for char in prefix:
			if not char in current:
				return False
			current = current[char]
		return True

	def search(self, word):
		cur = self.head
		for ch in word:
			if ch not in cur:
				return False
			cur = cur[ch]
		if '*' in cur:
			return True
		else:
			return False



mydict = Trie()
mydict.add('Trie')
mydict.add('startsWith')
print(mydict.startsWith(''))
print(mydict.startsWith('a'))